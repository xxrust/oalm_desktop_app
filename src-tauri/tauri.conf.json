{
  "$schema": "https://cdn.jsdelivr.net/gh/tauri-apps/tauri@2.5.0/tooling/cli/schema.json",
  "productName": "OLAM分析系统",
  "version": "1.0.0",
  "identifier": "com.olam.dev",
  "app": {
    "windows": [
      {
        "title": "OLAM分析系统",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "build": {
    "beforeDevCommand": "npm run dev:skipcheck",
    "beforeBuildCommand": "npm run build:skipcheck",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": ["app"],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    }
  },
  "plugins": {
    "http": {
      "all": true,
      "request": true,
      "scope": ["http://**", "https://**"]
    },
    "dialog": {
      "all": true
    },
    "fs": {
      "all": false,
      "readFile": true,
      "writeFile": true,
      "scope": ["$DOCUMENT/*", "$DESKTOP/*"]
    }
  }
}
